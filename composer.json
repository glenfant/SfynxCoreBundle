{
  "name": "sfynx-project/core-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides models of classes allowing to work and develop with doctrine (translation, tree, CRUD  generate Datatable table and multi-forms)",
  "keywords": ["DDD", "crud", "datatableJS", "multi-forms", "tree generalisation", "translation generalisation"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxCoreBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8|~3.0",
    "symfony/console": "*",
    "sensio/generator-bundle": "~3.1.2",
    "gedmo/doctrine-extensions": "~2.4.24",
    "doctrine/doctrine-cache-bundle": "~1.3.0",
    "stof/doctrine-extensions-bundle": "1.2.2",
    "sfynx-project/tool-trigger-bundle": "~2.8.1",
    "sfynx-project/tool-utils-bundle": "~2.8.1",
    "sfynx-project/tool-specification-bundle": "~2.8.3"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\CoreBundle": "" }
  },

  "target-dir": "Sfynx/CoreBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
